set -euo pipefail

PATH=/opt/homebrew/opt/grep/libexec/gnubin:$PATH

profiles() {
    grep profile ~/.aws/config | grep -oP '^\[profile \K.*' | tr -d ']' | while read -r profile; do
        cat &lt;&lt;EOF
    {"title": "$profile", "subtitle": "AWS profile", "arg": "$profile"},
EOF
    done | sed '$ s/,$//'
}

cat &lt;&lt;EOF
{"items": [
$(profiles)
]}
